# Example .env file for Plants Hub
# Copy this to .env and fill in values (DO NOT commit your real .env to git)

# Flask
FLASK_SECRET_KEY=change-me-in-production

# Database
# Example: postgresql+psycopg2://user:pass@host:port/dbname
DATABASE_URL=

# Cloudinary (set these to your Cloudinary dashboard values)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Other
PORT=5000
FLASK_DEBUG=1
